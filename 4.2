#include <iostream>
using namespace std;
inline int multiply(int a, int b) 
{
    return a * b;
}

int main() 
{
    int num1, num2;
    
    std::cout << "Enter first number: ";
    std::cin >> num1;
    
    std::cout << "Enter second number: ";
    std::cin >> num2;
    
    int result = multiply(num1, num2);
    
    std::cout << "Multiplication result: " << result << std::endl;
    
    return 0;
}